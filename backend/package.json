    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "onnxruntime-node": "^1.14.0",
    "opossum": "^8.1.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.7.2",
    "uuid": "^11.0.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
