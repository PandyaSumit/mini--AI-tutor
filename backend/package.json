{
  "name": "mini-ai-tutor-backend",
  "version": "1.0.0",
  "description": "Backend for Mini AI Tutor SaaS Platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "NODE_ENV=test jest --coverage --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit --coverage",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration --runInBand",
    "test:e2e": "NODE_ENV=test jest --testPathPattern=tests/e2e --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "mcp:start": "node ai/mcp/server.js",
    "seed:knowledge": "node scripts/seedKnowledgeBase.js",
    "admin:verify": "node scripts/verify-admin-setup.js",
    "admin:create": "node scripts/create-admin.js"
  },
  "keywords": [
    "ai",
    "tutor",
    "education",
    "saas"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/groq": "^0.1.0",
    "@langchain/langgraph": "^0.2.19",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/sdk-trace-node": "^1.28.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.0",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.9.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "groq-sdk": "^0.3.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
